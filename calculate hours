function calculateHours(a, b) {
  return a * b;
}
const result = calculateHours(8, 5);
console.log(result);

=========================


function calculateHours(days, hoursPerDay) {
  return days * hoursPerDay;
}

//you can put functions directly in console.logs

console.log(calculateHours(8, 5));

// or console logs in functions ;)

function calculateHours(a, b) {
  console.log(a*b);
  return a * b;
}
